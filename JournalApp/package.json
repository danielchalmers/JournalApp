{
  "name": "journalapp-theme",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "esbuild": "^0.21.5",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build:theme": "esbuild theme/material-theme.ts --bundle --format=esm --outfile=wwwroot/theme/material-theme.js --target=es2020",
    "watch:theme": "esbuild theme/material-theme.ts --bundle --format=esm --outfile=wwwroot/theme/material-theme.js --target=es2020 --watch",
    "typecheck:theme": "tsc --noEmit --pretty"
  }
}
